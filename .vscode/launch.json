    "version": "0.2.0",
    "configurations": 
    [       
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PS Deploy",
            "script": "${workspaceRoot}\\.vscode\\deploy.ps1",
            "args": [],
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8050/dreadhead/index.html"//,            "webRoot": "${workspaceRoot}/wwwroot"
        },
        {
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
            "file": "${workspaceRoot}/index.html"
        }
    ]   
}